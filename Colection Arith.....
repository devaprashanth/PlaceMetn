#include<stdio.h>
int main() 
  {
     int a,b,c,d,e,f;
     
     printf("ENTER A VALUE OF A :");
     scanf("%d",&a);
     printf("\nENTER A VALUE OF B :");
     scanf("%d",&b);
     
     c=a+b;
     
     d=a-b;
     
     e=a*b;

     f=a/b;
     
     printf("\nTHE ARITH ANSWER IS :");
     printf("\nADD =%d",c);
     printf("\nSUB =%d",d);
     printf("\nMUL =%d",e);
     printf("\nDIV https://www.onlinegdb.com/online_c_compiler#tab-stdin=%d",f);
     return 0;
      
      
    
}
